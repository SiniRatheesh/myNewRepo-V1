{
    "manifest_version": 2,
    "name": "Comikka",
    "description": "To view comics",
    "version": "1.0.0",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        "unlimitedStorage",
        "notifications"
    ],
    "short_name": "Comikka",
    "offline_enabled": false,
    "optional_permissions": [
        "notifications",
        "unlimitedStorage"
    ],
    "externally_connectable": {
        "ids": [],
        "matches": [],
        "accepts_tls_channel_id": false
    }
}